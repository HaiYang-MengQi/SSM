<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.experience.dao.User_Register_dao">
    <insert id="userAdd" parameterType="user">
        insert into user(user_name,user_password,sex,tel,address,word) values (#{user_name},#{user_password},#{sex},#{tel},#{address},#{word});
    </insert>
</mapper>