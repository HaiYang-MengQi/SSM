<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.experience.dao.User_login_dao">
    <select id="user_Login" parameterType="com.experience.entity.User" resultType="com.experience.entity.User">
            select * from user
            <where>
                user_name= #{user_name} and user_password = #{user_password}
            </where>
    </select>
</mapper>