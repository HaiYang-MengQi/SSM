<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.experience.dao.User_register_dao">
    <insert id="userAdd" parameterType="com.experience.entity.User">
        insert into user(username,password,sex,tel,address) values (#{username},#{password},#{sex},#{tel},#{address});
    </insert>
</mapper>